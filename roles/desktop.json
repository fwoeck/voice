{
  "name": "desktop",
  "description": "local Voice dev setup",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "etc": {
      "railsenv":       "development",
      "default_branch": "develop"
    },
    "wim": {
      "user":    "vagrant",
      "group":   "vagrant",
      "home":    "/home/vagrant",
      "gitbase": "git@github.com:fwoeck",
      "branch":  "develop",
      "color":   "green"
    },
    "rvm": {
      "basedir": "/home/vagrant/.rvm"
    },
    "jruby": {
      "opts": "-J-Djruby.cext.enabled=false -J-Djruby.compat.version=1.9 -J-Dfile.encoding=UTF-8"
    }
  },
  "chef_type":   "role",
  "run_list": [
    "role[server]",
    "recipe[wim-user-dev]",
    "recipe[phantomjs]",
    "recipe[v4l2]"
  ],
  "env_run_lists": {
  }
}


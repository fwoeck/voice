{
  "name":        "desktop",
  "description": "local Voice dev setup",
  "json_class":  "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "etc": {
      "railsenv":       "development",
      "default_branch": "develop"
    },
    "wim": {
      "user":   "vagrant",
      "group":  "vagrant",
      "home":   "/home/vagrant",
      "branch": "develop",
      "color":  "green"
    },
    "rvm": {
      "basedir": "/home/vagrant/.rvm"
    }
  },
  "chef_type":   "role",
  "run_list": [
    "role[server]",
    "recipe[wim-user-dev]",
    "recipe[phantomjs]"
  ],
  "env_run_lists": {
  }
}

